				//default: false
				var centerPadding;
				//default: 50px
				var slide;
				//default: ?
				var breakpoints = [];
				var responsive = [];
				var variableWidth;
				//default: false
				var cssEase;
				//default: ease
				var responsiveSettings = {
					'arrows' : arrows,
					'centerMode' : false,
					'centerPadding' : '40px',
					'variableWidth' : false,
					slidesToShow : 1,
				};

				breakpoints.forEach(function(point) {
					responsive.push({
						'breakpoint' : point,
						'settings' : responsiveSettings
					});
				});

			} else {
				if (!!$(this).hasAncestor('.container-fluid')) {
					$(this).slick({
						dots : true,
						infinite : true,
						speed : 500,
						fade : false,
						slide : 'li',
						cssEase : 'linear',
						centerMode : true,
