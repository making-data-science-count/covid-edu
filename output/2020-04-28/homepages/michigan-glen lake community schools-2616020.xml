	var category_options = document.querySelectorAll('.events-category-tab');
	var event_conts;
	var max_events = 5;

	function show_cal_category(el, index){
		for(var i = 0; i < category_options.length; i++){
			category_options[i].className = category_options[i].className.replace(/(^|\s)on($|\s)/, '');
			event_conts[i].className = event_conts[i].className.replace(/(^|\s)on($|\s)/, '');
		}

		el.className += ' on';
		document.getElementById('events_'+index).className += ' on';

		var messageCont = document.getElementById('event-subscribe-message');
		if(index === 0){
			messageCont.style.display = '';									
		}
		else {
			messageCont.style.display = 'none';
		}
	}

	function get_cal_data(max_events){
		var url = document.getElementsByTagName("base")[0].href + "form-proc-get-calendar-events.html";
		url += '?max=' + max_events;

	    $.ajax({
	        type: "GET",
	        url: url,
	        datatype: 'json',
	        contentType: 'application/json; charset=utf-8',
	        success: function (result) {
	        	if (result) {
	        		var return_data = JSON.parse(result);

	        		document.getElementById('events-cont').innerHTML = return_data.html;
	        		event_conts = document.querySelectorAll('.category-events-cont');
	        	}
	        },
	        error: function (result) {
	        }
	    });	
	}

	get_cal_data(max_events);


</script>
