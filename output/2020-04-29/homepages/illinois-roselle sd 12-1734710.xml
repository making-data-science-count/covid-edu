        arrows: false,
        slidesToShow: 1
    });


</script><script>
    // Get the modal
    var modal = document.getElementById('newsletterModal');

    // Get the button that opens the modal
    var btn = document.getElementById("aShowNewsletter");

    // Get the <span> element that closes the modal
    var span = document.getElementsByClassName("modal_close")[0];

    var iFrame = document.getElementById("newsletterFrame");

    // When the user clicks the button, open the modal 
    btn.onclick = function () {
        modal.style.display = "block";
        iFrame.src = iFrame.src;
    }

    // When the user clicks on <span> (x), close the modal
    span.onclick = function () {
        modal.style.display = "none";
    }

    // When the user clicks anywhere outside of the modal, close it
    window.onclick = function (event) {
        if (event.target == modal) {
            modal.style.display = "none";
        }
    }

    // When the user hits escape, close the modal if it is open
    $(document).keyup(function (e) {
        if (e.keyCode == 27) { // escape key maps to keycode `27`
            if (modal.style.display == "block") {
                modal.style.display = "none";
            }
        }
    });
</script>
