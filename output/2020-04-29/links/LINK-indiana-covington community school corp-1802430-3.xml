<script>
  $(document).ready(function()
  {
    var $control = $("#testionial-control");
    var $control_label = $control.find('.testionial-control-label');
    var $control_icon = $control.find('.testionial-control-icon');
    var carousel_state = 0;

    $control.on('click', function()
    {
      carousel_state = !carousel_state;
      if (carousel_state)
      {
        $('#testimonial-carousel').carousel('pause');
        $control.attr('title', 'Play Testimonials');
        $control.attr('aria-label', 'Play Testimonials');
        $control_label.html('Play');
        $control_icon.removeClass('fa-pause');
        $control_icon.addClass('fa-play');
      }
      else
      {
        $('#testimonial-carousel').carousel('cycle');
        $('#testimonial-carousel').carousel('next');
        $control.attr('title', 'Pause Testimonials');
        $control.attr('aria-label', 'Pause Testimonials');
        $control_label.html('Pause');
        $control_icon.removeClass('fa-play');
        $control_icon.addClass('fa-pause');
      }
    });
  });
</script>
