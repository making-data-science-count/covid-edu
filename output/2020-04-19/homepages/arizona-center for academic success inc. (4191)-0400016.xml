function Validator(frm)
{
 var btnSearch = ("Name");
 
 switch (btnSearch)
   {
  case 'Name':
        var elem = frm.elements("SiteName")
        break;
  case 'City':        
        var elem = frm.elements("City")
        break;
  case 'County':
        var elem = frm.elements("County")
        break;
  case 'Postal':
        var elem = frm.elements("ZipCode")
        break;
  case 'CTDS':
        var elem = frm.elements("CTDS")
        break;  
  default:
        return (false);
  }
  
  if (elem != null) {   
      if (elem.value == "") {
         alert("Please enter a valid entry.");
         elem.focus();
         return (false);
      }
  }
  return (true);
}
