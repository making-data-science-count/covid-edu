<script>
$(function() {
    $("#user_suggest_1691").autocomplete({
        source: function( request, response ) {
            $.ajax({
                url: '/includes/ajax_user_suggest.php',
                dataType: "json",
                data: { user_suggest: request.term },
                success: function(data) {
                    if (data.results < 1) {
                        $("#user_suggest_1691").autocomplete("close");
                    } else {
                        response($.map(data.matches, function(item) {
                            return {
                                label: item.display_name, 
                                user_id: item.user_id
                            }
                        }));
                    }
                }
            });
        },
        delay: 300, 
        minLength: 1,
        autoFocus: true, 
        select: function(event, ui) {
            $.ajax({
                url: '/includes/w_update_employee_directory.php',
                type: 'POST',
                data: { user_id: ui.item.user_id, rand_id: '1691' },
                success: function(data) {
                    $('#w_user_details_1691').html(data).slideDown('fast');
                }
            });
        }
    });
});
</script><input id="user_suggest_1691" style="margin-top: 6px; font-size: 14px; margin-bottom: 6px; padding: 3px; width: 210px;"><div id="w_user_details_1691" class="hidden"></div>
