	.map-by-testing div:after {content:"More Testing"; display:inline-block; position: absolute; right:-75px;}
			
			
			
			
			.disclaimer {position: fixed; bottom: 0; left: 0; background:#fff; padding: 10px; font-size: .8em; width: 250px; border-right: 1px solid #ccc; background:#f5f5f5;}
