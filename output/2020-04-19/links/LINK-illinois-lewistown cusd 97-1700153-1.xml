.alertcenter-banner {
  background: #ca2221; /* Old browsers */
  background: -moz-linear-gradient(top, #6e130d 0%, #ca2221 49%, #8b0c0b 100%); /* FF3.6-15 */
  background: -webkit-linear-gradient(top, #6e130d 0%, #ca2221 49%, #8b0c0b 100%); /* Chrome10-25,Safari5.1-6 */
  background: linear-gradient(to bottom, #6e130d 0%, #ca2221 49%, #8b0c0b 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  border-bottom: solid 1px #6e0c08;
  color: #fff;
  font-size: 16px;
  font-weight: bold;
  padding: 8px 0;
  text-align: center;
  text-transform: uppercase;
}
.alertcenter-banner a {
  color: inherit;
  text-decoration: none;
}
.alertcenter-banner .close {
  background-color: white;
  border-radius: 50%;
  color: #ca2221;
  float: right;
  font-family: arial, sans-serif;
  font-size: 18px;
  font-weight: bold;
  height: 20px;
  width: 20px;
  letter-spacing: normal;
  line-height: 20px;
  margin-right: 8px;
  opacity: .8;
  transition: .2s opacity;
}
.alertcenter-banner .close:focus,
.alertcenter-banner .close:hover {
  opacity: 1;
}
.menu-alerts a {
  background-color: #ca2221;
  color: #fff!important;
  font-weight: bold;
}
.menu-alerts a:focus,
.menu-alerts a:hover {
  color: #fff!important;
}
