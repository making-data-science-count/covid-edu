<script type="text/javascript">
(function($){
  $(function() {

    var ie = ($.browser.msie); // Why? Because IE sucks
    var $eventTds = $('#calendarMonth td.event');
    
    // No events = nothing to do
    if ($eventTds.length === 0) return false;

    // Hide all the event balloons
    $eventTds.each(function() {
      $('div.eventBalloon', this).css('opacity', 0);
      
      // IE Hack: Add an iframe "shim" that will be displayed underneath the balloon 
      // to keep links underneath the balloon from interferring with the balloon.
      if (ie) $(this).append('<iframe style="display:none;position:absolute;z-index:2;"></iframe>');
    });

    $eventTds
      .hoverIntent({
        timeout: 200,
        over: function(e) {
          var $td = $(this);
          var offset = $td.offset();
          var pos = $td.position();
          var $div = $('div.eventBalloon', this);
          
          // Keep the balloon on screen
          var top = pos.top;
          var winBottomEdge = $(window).height() + $(window).scrollTop() - 10;
          if (offset.top + $div.height() > winBottomEdge) {
            top = winBottomEdge - $div.height() - (offset.top - top);
            $div.addClass('rounded');
          } else {
            $div.removeClass('rounded');
          }
          
          $div.css({
              'left': pos.left - $div.width() + 8, // Pull it in from the edge a bit with the '8'
              'top': top
          });
          
          $div.stop().animate({'opacity': 1}, function() {
            if (ie) {
              var divPos = $div.position();
              $td.find('iframe').css({
                'display': 'block',
                'left': pos.left - $div.width() + 8,
                'top': divPos.top,
                'height': $div.height(),
                'width': $div.width()
              });
            }
          });
        },
        out: function() {
          if (ie) $(this).find('iframe').hide();
          $('div.eventBalloon', this)
            .stop()
            .animate({'opacity': 0}, 'fast', function() {
              $(this).css('left', '').find('a').blur(); // Get rid of the "outline" around clicked anchors
            });
        }
      });

  });
})(jQuery);
</script>
